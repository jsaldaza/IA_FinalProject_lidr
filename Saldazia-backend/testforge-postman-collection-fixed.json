    {
      "name": "Auth - Login",
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "// Parse response and set authToken in environment",
              "var json = {};",
              "try { json = pm.response.json(); } catch (e) { json = {}; }",
              "var token = null;",
              "if (json) {",
              "  token = json.token || (json.data && (json.data.token || json.data.accessToken)) || json.accessToken || json.access_token;",
              "}",
              "if (token) {",
              "  pm.environment.set('authToken', token);",
              "  console.log('authToken saved to environment');",
              "} else {",
              "  console.log('No token found in response:', json);",
              "}",
              "pm.test('Login response status is 2xx', function () { pm.response.to.be.success; });"
            ]
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"{{EMAIL}}\",\n  \"password\": \"{{PASSWORD}}\"\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": {
          "raw": "{{HOST}}/api/auth/login",
          "protocol": "http",
          "host": [
            "{{HOSTNAME}}"
          ],
          "port": "{{HOST_PORT}}",
          "path": [
            "api",
            "auth",
            "login"
          ]
        }
      }
